<h1><%= @doctor.name%></h1>
<% @doctor.appointments.each do |appointment| %>
  <% @doctor_patient = @doctor.patients.find(appointment.patient_name) %>
  <%= link_to @doctor_patient.name, patient_path(@doctor_patient) %> - <%= link_to appointment.appointment_datetime.strftime("%B %d, %Y"), appointment_path(appointment) %><br>
<% end %>
